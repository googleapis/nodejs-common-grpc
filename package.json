    "is": "^3.2.0",
    "modelo": "^4.2.0",
    "retry-request": "^3.3.1",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^2.1.3",
